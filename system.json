{
    "Name":"Constellinux",
    "Author":"ThatBeaverDev",
    "Version":"0.0.3",
    "startup":[
        "/sys/crl/desktop.crl",
        "/sys/crl/dock.crl",
        "/sys/crl/files.crl"
    ],
    "roturAuth":true,
    "startAppShortcut":true,
    "files":[
        {
            "location":"/files.crl",
            "directory":"/sys/crl/files.crl"
        },
        {
            "location": "/desktop.crl",
            "directory": "/sys/crl/desktop.crl"
        },
        {
            "location":"/dock.crl",
            "directory":"/sys/crl/dock.crl"
        }
    ],
    "specialperms":{
        "/sys/crl/desktop.crl":[
            "sysr"
        ]
    }
}